package com.zensar.entity;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "stocks")

public class StockEntity {
    @Id
    private int id;

    private String name;

    private String market;

    private int price;

    public StockEntity() {
	// TODO Auto-generated constructor stub
    }

    public StockEntity(int id, String name, String market, int price) {
	super();
	this.id = id;
	this.name = name;
	this.market = market;
	this.price = price;
    }

    public int getId() {
	return id;
    }

    public void setId(int id) {
	this.id = id;
    }

    public String getName() {
	return name;
    }

    public void setName(String name) {
	this.name = name;
    }

    public String getMarket() {
	return market;
    }

    public void setMarket(String market) {
	this.market = market;
    }

    public int getPrice() {
	return price;
    }

    public void setPrice(int price) {
	this.price = price;
    }

    @Override
    public String toString() {
	return "Stock [name=" + name + ", market=" + market + ", price=" + price + "]";
    }

}